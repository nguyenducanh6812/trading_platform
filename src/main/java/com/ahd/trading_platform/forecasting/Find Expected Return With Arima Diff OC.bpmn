  <bpmn:collaboration id="Collaboration_09th02d">
    <bpmn:participant id="Participant_0a2a3uz" name="Calculate Expected Return By Arima Diff OC" processRef="Process_0hntsrr" />
    <bpmn:textAnnotation id="TextAnnotation_0czw3vu">
      <bpmn:text>Input:
1. InstrumentCode: BTC, ETH...
2. Dates: Dates want to find expected result</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:textAnnotation id="TextAnnotation_0b750kx">
      <bpmn:text>OC: Open - Close
Diff_OC: OC(T) - OC(T-1)
Load Mean_Diff_OC from config
Demean_Diff_OC: Diff_OC - Mean_Diff_OC</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:textAnnotation id="TextAnnotation_1edhq5a">
      <bpmn:text>Ar.L1 (T) = Demean_Diff_OC (T-1)
Ar.L2 (T) = Demean_Diff_OC (T-2)
...
Ar.LN (T) = Demean_Diff_OC (T-N)

Prd_Diff_OC(T) = Σ[Ar.L(i) × ARIMA.Coefficient(i)] + Mean_Diff_OC

Prd_OC(T) = Prd_Diff_OC(T) + OC(T-1) 

Prd_Return(T) = Prd_OC(T) / Open_Price(T)</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:association id="Association_0wwkyij" associationDirection="None" sourceRef="StartEvent_1" targetRef="TextAnnotation_0czw3vu" />
  </bpmn:collaboration>
